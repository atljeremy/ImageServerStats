<table>
  <thead>
    <tr>
      <th>Image</th>
      <th><a href="?sort=cache_hit">Cache Hit</a></th>
      <th><a href="?sort=processing_time">Server Processing Time</a></th>
      <th><a href="?sort=download_time">Download Time</a></th>
      <th><a href="?sort=size">Size</a></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <th><%= image_tag "http://image.apartmentguide.com/#{photo.server_url}", width: '75' %></th>
        <td width='100'><%= photo.cache_hit %></td>
        <td width='200'><b><%= photo.processing_time.round(2) if photo.processing_time %> seconds</b></td>
        <td width='200'><b><%= photo.download_time.round(4) %> seconds</b></td>
        <td width='100'><b><%= photo.size %> bytes</b></td>
        <td><%= link_to 'Show', photo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
